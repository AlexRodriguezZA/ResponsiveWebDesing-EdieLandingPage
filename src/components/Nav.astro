---
const navItems = [
  { label: "Home" },
  { label: "Services" },
  { label: "Our Works" },
  { label: "Clients" },
  { label: "Contact" },
];
---

<nav class="w-11/12 md:w-full  max-w-5xl h-24 flex justify-between items-center text-[#333]">
  <ul>
    <li class="text-2xl  md:text-3xl font-extrabold">Edie</li>
  </ul>
  <ul class="gap-10 hidden md:flex">
    {
        navItems.map( item => <li class="font-medium text-xl hover:cursor-pointer">{item.label} </li> )
    }
  </ul>
</nav>
